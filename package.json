{
  "name": "spotifyapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "v18.13.0"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "server": "nodemon index.js",
    "client": "cd client && npm start",
    "postinstall": "cd client && npm install -production",
    "build": "NODE_ENV=production cd client/ && npm install -production && npm run build && modclean -r -n default:safe",
    "start-server": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuilfanie/SpotifyApp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vuilfanie/SpotifyApp/issues"
  },
  "homepage": "https://github.com/vuilfanie/SpotifyApp#readme",
  "dependencies": {
    "axios": "^1.2.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
